{
  "name": "electron-sample",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron dist",
    "package": "electron-packager dist Sample --platform=darwin,win32 --arch=x64 --version=1.2.7 --overwrite",
    "copy:json": "cpx \"package.json\" dist --verbose",
    "copy:html:development": "cpx \"src/index.html\" dist --verbose",
    "copy:html:production": "node node_modules/useref-cli/index.js src/index.html dist",
    "build:development": "cross-env NODE_ENV=development npm-run-all build:main build:renderer copy:json copy:html:development",
    "build:production": "cross-env NODE_ENV=production npm-run-all build:main build:renderer copy:json copy:html:production",
    "build:main": "tsc --project ./config/main.tsconfig.json --outDir dist",
    "build:renderer": "webpack --config ./config/renderer.webpack.config.js",
    "develop": "cross-env NODE_ENV=development npm-run-all build:development --parallel watch:tsc:main watch:webpack:renderer livereload",
    "watch:tsc:main": "tsc --project ./config/main.tsconfig.json --outDir dist --watch",
    "watch:webpack:renderer": "webpack --config ./config/renderer.webpack.config.js --watch",
    "livereload": "gulp"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "core-js": "2.4.0",
    "cpx": "1.3.1",
    "cross-env": "2.0.0",
    "electron-packager": "7.3.0",
    "electron-prebuilt": "1.2.7",
    "npm-run-all": "2.3.0",
    "raw-loader": "0.5.1",
    "rxjs": "5.0.0-beta.6",
    "ts-loader": "0.8.2",
    "typescript": "1.8.10",
    "useref-cli": "1.0.0",
    "webpack": "1.13.1",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "electron-connect": "0.4.2",
    "gulp": "3.9.1"
  }
}
